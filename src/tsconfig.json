{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": false,
		"allowJs": true,
		"resolveJsonModule": true,
		"isolatedModules": false,
		"outDir": "../out/cd2030"
	},
	"include": [
		"./*.ts",
		"./typings/**/*.d.ts",
		"./**/*.ts"
	]
}
